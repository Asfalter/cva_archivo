<p id="notice"><%= notice %></p>

<h1>Listing Estados Bloques</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estados_bloques.each do |estados_bloque| %>
      <tr>
        <td><%= estados_bloque.descripcion %></td>
        <td><%= link_to 'Show', estados_bloque %></td>
        <td><%= link_to 'Edit', edit_estados_bloque_path(estados_bloque) %></td>
        <td><%= link_to 'Destroy', estados_bloque, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Estados bloque', new_estados_bloque_path %>
