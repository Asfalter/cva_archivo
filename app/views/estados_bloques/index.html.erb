<p id="notice"><%= notice %></p>

<div class="panel panel-default">
	<div class="panel-heading">
		<div class="panel-title">
			Estados de Bloque
		</div>
	</div>
	<div class="panel-body">
		<table class="table table-condensed table-hover">
		  <thead>
		    <tr>
		      <th>Descripcion</th>
		      <th colspan="3"></th>
		    </tr>
		  </thead>

		  <tbody>
		    <% @estados_bloques.each do |estados_bloque| %>
		      <tr>
		        <td><%= estados_bloque.descripcion %></td>
		        <td><%= link_to 'Show', estados_bloque %></td>
		        <td><%= link_to 'Edit', edit_estados_bloque_path(estados_bloque) %></td>
		        <td><%= link_to 'Destroy', estados_bloque, method: :delete, data: { confirm: 'Are you sure?' } %></td>
		      </tr>
		    <% end %>
		  </tbody>
		</table>
	</div>
	<div class="panel-footer">
		<%= link_to 'Nuevo Estado de Bloque', new_estados_bloque_path %> |
		<%= link_to 'Regresar', archivo_path %>
	</div>
</div>


