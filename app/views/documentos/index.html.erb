<p id="notice"><%= notice %></p>

<h1>Listing Documentos</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th>Descripcion</th>
      <th>Estado</th>
      <th>Localidad</th>
      <th>Ubicacion</th>
      <th>Custodio</th>
      <th>Bloque</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documentos.each do |documento| %>
      <tr>
        <td><%= documento.tipo %></td>
        <td><%= documento.descripcion %></td>
        <td><%= documento.estado %></td>
        <td><%= documento.localidad %></td>
        <td><%= documento.ubicacion %></td>
        <td><%= documento.custodio %></td>
        <td><%= documento.bloque %></td>
        <td><%= link_to 'Show', documento %></td>
        <td><%= link_to 'Edit', edit_documento_path(documento) %></td>
        <td><%= link_to 'Destroy', documento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Documento', new_documento_path %>
